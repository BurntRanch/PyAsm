$ Include the sha256 function from the hashlib library
INCLUDE sha256 FROM hashlib;
INCLUDE sys;

$ Define the print function
DEFINE FUNC print;

    $ CALL input ARGS ("input: ");

    SET RETURN TO "hi";

    $ Hash input
    CALL sha256 ARGS (RETURN.encode());

    $ hex digest it
    CALL RETURN.hexdigest;

    SET hash TO RETURN;

    SET counter TO 0;

    SET exit TO False;
    
    $ if input is equal to the hash of "hi"
    IF hash == "8f434346648f6b96df89dda901c5176b10a6d83961dd3c1ac88b59b2dc327aa4";

        WHILE counter < 10;

            $ add one to it and call sys.stdout.write
            INCREMENT counter BY 1;

            CALL sys.stdout.write ARGS (str(counter) + '\n');

        ENDWHILE;

        $ yes
        CALL sys.stdout.write ARGS ("ok im gonna break now\n");

    ENDIF;

    CALL sys.exit;

$ End of function definition
EXIT FUNC;
